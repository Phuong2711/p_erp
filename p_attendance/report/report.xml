<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="attendance_xlsx_report" model="ir.actions.report">
        <field name="name">Export Attendance</field>
        <field name="model">report.p_attendance.attendance_xlsx</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">p_attendance.attendance_xlsx</field>
        <field name="report_file">p_attendance.attendance_xlsx</field>
        <field name="print_report_name">'Attendance %s_%s' % (object.month, object.year)</field>
    </record>
</odoo>